<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coronavirusapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EmergencyContact"
            android:label="@string/title_activity_emergency_contact"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".UpdateHealthStatus" />
        <activity android:name=".AlmostThere" />
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OTPVerification" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>